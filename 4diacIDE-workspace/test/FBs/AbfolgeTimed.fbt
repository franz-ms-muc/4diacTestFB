			<Event Name="EO1" Type="Event" Comment="">
				<With Var="DO1"/>
				<With Var="ActualStep"/>
			</Event>
			<Event Name="EO2" Type="Event" Comment="">
				<With Var="DO2"/>
				<With Var="ActualStep"/>
			</Event>
